<?xml version="1.0"?>
<Container version="2">
  <Name>flatnotes</Name>
  <Repository>dullage/flatnotes</Repository>
  <Registry>https://hub.docker.com/r/dullage/flatnotes</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/138851-support-flatnotes/</Support>
  <Project>https://github.com/Dullage/flatnotes</Project>
  <Overview>A self-hosted, database-less note taking web app that utilises a flat folder of markdown files for storage.&#xD;
&#xD;
Based on the TQ templete, edited to match my youtube series config.</Overview>
  <Category>Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/tquizzle/Docker-xml/master/docker-flatnotes.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/riffsphereha/Unraid-Templates/main/flatnotes/flatnotes.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1683794509</DateInstalled>
  <DonateText>If you like my work please consider Donating.</DonateText>
  <DonateLink>https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&amp;amp;hosted_button_id=KK2S75LBHM6QC&amp;amp;source=url_id=JTSH886FD9UM6</DonateLink>
  <Requires/>
  <Config Name="Auth Type" Target="FLATNOTES_AUTH_TYPE" Default="password" Mode="" Description="" Type="Variable" Display="always-hide" Required="true" Mask="false">password</Config>
  <Config Name="Username" Target="FLATNOTES_USERNAME" Default="user" Mode="" Description="" Type="Variable" Display="always-hide" Required="true" Mask="false">user</Config>
  <Config Name="Password" Target="FLATNOTES_PASSWORD" Default="changeMe!" Mode="" Description="" Type="Variable" Display="always-hide" Required="true" Mask="true">password</Config>
  <Config Name="Secret Key" Target="FLATNOTES_SECRET_KEY" Default="aLongRandomSeriesOfCharacters" Mode="" Description="" Type="Variable" Display="always-hide" Required="true" Mask="false">aLongRandomSeriesOfCharacters</Config>
  <Config Name="WebUI" Target="8080" Default="8080" Mode="tcp" Description="WebUI HTTP" Type="Port" Display="always-hide" Required="true" Mask="false">1002</Config>
  <Config Name="Appdata" Target="/data" Default="/mnt/user/appdata/flatnotes" Mode="rw" Description="Container Path: /data&#13;&#10;Contains all relevant configuration files." Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/flatnotes</Config>
  <Config Name="User ID" Target="PUID" Default="99" Mode="" Description="" Type="Variable" Display="advanced-hide" Required="true" Mask="false">99</Config>
  <Config Name="Group ID" Target="GUID" Default="100" Mode="" Description="" Type="Variable" Display="advanced-hide" Required="true" Mask="false">100</Config>
</Container>