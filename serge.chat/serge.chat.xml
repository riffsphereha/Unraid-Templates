<?xml version="1.0"?>
<Container version="2">
  <Name>Serge.chat</Name>
  <Repository>ghcr.io/nsarrazin/serge:latest</Repository>
  <Registry>https://github.com/nsarrazin/serge/pkgs/container/serge</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/139096-support-riffspheres-templates/</Support>
  <Project>https://serge.chat/</Project>
  <Overview>Your self-hosted personal assistant.&#xD;
No remote API, no data collection, no ads.</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:8008]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/riffsphereha/Unraid-Templates/blob/main/serge.chat/icon-symbol-chat-outline-illustration-free-vector.jpg?raw=true</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1685990509</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WebUI" Target="8008" Default="" Mode="tcp" Description="" Type="Port" Display="always-hide" Required="false" Mask="false">8008</Config>
  <Config Name="datadb" Target="/data/db/" Default="" Mode="rw" Description="" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/serge/db</Config>
  <Config Name="weights" Target="/usr/src/app/weights" Default="" Mode="rw" Description="" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/serge/weights</Config>
</Container>